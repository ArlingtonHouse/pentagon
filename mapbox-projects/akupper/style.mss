#districts { line-color: #999; line-width: 0.5; polygon-opacity: 0.5; }
@c1: #f3b05d;
@c2: #b3bf7e;
@c3: #8cbcb5;
@c4: #e1582c;
@c5: #5e6f3e;
@c6: #698784;
@c7: #932700;
@c8: #7f985e;
@c9: #204e50;


#districts[SLDUST="ZZZ"] { polygon-opacity: 0; line-opacity: 0; }


#districts[SLDUST="00A"] { polygon-fill: @c1; }
