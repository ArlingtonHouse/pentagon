#districts[SLDUST="00H"] { polygon-fill: @c5; }
#districts[SLDUST="00I"] { polygon-fill: @c6; }
#districts[SLDUST="00J"] { polygon-fill: @c1; }
#districts[SLDUST="00K"] { polygon-fill: @c2; }
#districts[SLDUST="00L"] { polygon-fill: @c3; }
#districts[SLDUST="00M"] { polygon-fill: @c4; }
#districts[SLDUST="00N"] { polygon-fill: @c6; }
